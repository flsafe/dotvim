"GUI Options"
if has("gui_gtk2")
  set guifont=Courier\ New\ 13
  set guifont=Mensch\ 13
  set guifont=DejaVu\ Sans\ Mono\ 10
  behave mswin
else
  set guifont=Inconsolata:h20
endif
set guioptions-=T
set guioptions-=m
set guioptions-=r

